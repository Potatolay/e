local function syncHRPVel(Instance_, Character)				
    local MoveVelocity = Vector3.new(0, 0, 0)
    local BodyForce = Instance.new("BodyForce", Instance_)

    local Signal = game.RunService.Heartbeat:connect(function()
        Instance_.Velocity = MoveVelocity
        
        
        Character.Torso.Running:Connect(function(Speed)
            if Speed > 0 then 
                Nothing = "pass"
            else
                Instance_.Velocity = Vector3.new(-37.3,0,-37.3)
            end
        end)
    end)
    
    game.RunService.Stepped:Connect(function()
		BodyForce.Force = Instance_.CFrame.Position
	end)
end

